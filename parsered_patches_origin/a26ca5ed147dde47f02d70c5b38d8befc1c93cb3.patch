-  src->rm_ = scratch;
-
-  DCHECK(is_int16(src->offset()));
-  if (two_accesses) {
-    DCHECK(is_int16(
-        static_cast<int32_t>(src->offset() + second_access_add_to_offset)));
-  }
-  DCHECK(misalignment == (src->offset() & (kDoubleSize - 1)));
-}
