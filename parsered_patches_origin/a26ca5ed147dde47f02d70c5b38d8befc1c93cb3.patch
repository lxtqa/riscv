-      break;
-    case MOD_S:
-      res = wt != 0 ? ws % wt : static_cast<T>(Unpredictable);
-      break;
-    case MOD_U:
-      res = wt != 0 ? static_cast<T>(static_cast<uT>(ws) % static_cast<uT>(wt))
