-      // GPR rd is 32.
-      alu_out = base::bits::CountLeadingZeros32(rs_u());
-      break;
-    default:
-      alu_out = 0x12345678;
-      UNREACHABLE();
-  }
-  SetResult(rd_reg(), alu_out);
