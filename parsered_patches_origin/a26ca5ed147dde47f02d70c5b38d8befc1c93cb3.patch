-  if (this->OpcodeFieldRaw() != SPECIAL) {
-    return false;
-  } else {
-    switch (this->FunctionFieldRaw()) {
-      case BREAK:
-      case TGE:
-      case TGEU:
-      case TLT:
-      case TLTU:
-      case TEQ:
-      case TNE:
