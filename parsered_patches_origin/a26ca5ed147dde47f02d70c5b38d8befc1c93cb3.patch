-inline LiftoffCondition cond_make_unsigned(LiftoffCondition cond) {
-  switch (cond) {
-    case kSignedLessThan:
-      return kUnsignedLessThan;
-    case kSignedLessEqual:
-      return kUnsignedLessEqual;
-    case kSignedGreaterThan:
