-  InstructionOperand inputs[] = {g.UseRegister(base), g.UseRegister(index),
-                                 g.UseFixed(value, a1),
-                                 g.UseFixed(value_high, a2)};
-  InstructionOperand outputs[2];
-  size_t output_count = 0;
-  InstructionOperand temps[3];
-  size_t temp_count = 0;
-  temps[temp_count++] = g.TempRegister(a0);
