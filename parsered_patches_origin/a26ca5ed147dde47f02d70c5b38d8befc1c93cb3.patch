-  Ctz(dst.low_gp(), src.low_gp());
-  jmp(&done);
-
-  bind(&low_is_zero);
-  Ctz(dst.low_gp(), src.high_gp());
-  Addu(dst.low_gp(), dst.low_gp(), Operand(32));
