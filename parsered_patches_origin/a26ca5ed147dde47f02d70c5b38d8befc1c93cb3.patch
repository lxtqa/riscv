-    case DOTP_U:
-    case DPADD_S:
-    case DPADD_U:
-    case DPSUB_S:
-    case DPSUB_U:
-    case SLD:
-    case SPLAT:
-      UNIMPLEMENTED();
-      break;
-    case SRAR: {
-      int bit = wt_modulo == 0 ? 0 : (ws >> (wt_modulo - 1)) & 1;
