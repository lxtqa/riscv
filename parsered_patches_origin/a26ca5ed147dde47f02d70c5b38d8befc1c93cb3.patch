-void RegExpMacroAssemblerMIPS::AdvanceRegister(int reg, int by) {
-  DCHECK_LE(0, reg);
-  DCHECK_GT(num_registers_, reg);
-  if (by != 0) {
-    __ lw(a0, register_location(reg));
-    __ Addu(a0, a0, Operand(by));
-    __ sw(a0, register_location(reg));
-  }
-}
