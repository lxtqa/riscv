-    if (read_backward) {
-      __ Subu(current_input_offset(), current_input_offset(), Operand(s3));
-    } else {
-      __ Addu(current_input_offset(), current_input_offset(), Operand(s3));
-    }
-  }
-
-  __ bind(&fallthrough);
-}
