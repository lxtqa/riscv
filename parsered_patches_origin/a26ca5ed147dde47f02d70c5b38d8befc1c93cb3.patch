-void CodeGenerator::FinishFrame(Frame* frame) {
-  auto call_descriptor = linkage()->GetIncomingDescriptor();
-
-  const DoubleRegList saves_fpu = call_descriptor->CalleeSavedFPRegisters();
-  if (!saves_fpu.is_empty()) {
