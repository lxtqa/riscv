-    __ JumpIfSmi(a3, &new_target_not_constructor);
-    __ lw(t1, FieldMemOperand(a3, HeapObject::kMapOffset));
-    __ lbu(t1, FieldMemOperand(t1, Map::kBitFieldOffset));
-    __ And(t1, t1, Operand(Map::Bits1::IsConstructorBit::kMask));
