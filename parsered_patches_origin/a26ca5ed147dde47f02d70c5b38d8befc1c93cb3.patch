-#endif
-  auto f = GeneratedCode<F3>::FromCode(*code);
-
-  (f.Call(&res, 0, 0, 0, 0));
-
-  CHECK_EQ(input->exp_res_lo, res.d[0]);
-  CHECK_EQ(input->exp_res_hi, res.d[1]);
-}
