-    } break;
-    case SRLR: {
-      uT wsu = static_cast<uT>(ws);
-      int bit = wt_modulo == 0 ? 0 : (wsu >> (wt_modulo - 1)) & 1;
-      res = static_cast<T>((wsu >> wt_modulo) + bit);
-    } break;
-    default:
-      UNREACHABLE();
-  }
-  return res;
-}
