-void Assembler::c(FPUCondition cond, SecondaryField fmt, FPURegister fs,
-                  FPURegister ft, uint16_t cc) {
-  DCHECK(is_uint3(cc));
-  DCHECK(fmt == S || fmt == D);
-  DCHECK_EQ(fmt & ~(31 << kRsShift), 0);
-  Instr instr = COP1 | fmt | ft.code() << 16 | fs.code() << kFsShift | cc << 8 |
-                3 << 4 | cond;
-  emit(instr);
