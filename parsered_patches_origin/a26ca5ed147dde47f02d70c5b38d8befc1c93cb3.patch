-                         FPURegisters::Name(fpuregnum + 1),
-                         FPURegisters::Name(fpuregnum), lvalue1, lvalue2,
-                         dfvalue);
-                }
-              }
-            } else {
-              PrintF("%s unrecognized\n", arg1);
-            }
-          }
-        } else {
-          if (argc == 3) {
-            if (strcmp(arg2, "single") == 0) {
-              int32_t value;
-              float fvalue;
-              int fpuregnum = FPURegisters::Number(arg1);
-
-              if (fpuregnum != kInvalidFPURegister) {
