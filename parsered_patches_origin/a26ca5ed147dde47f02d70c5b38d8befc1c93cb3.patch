-inline FPUCondition ConditionToConditionCmpFPU(LiftoffCondition condition,
-                                               bool* predicate) {
-  switch (condition) {
-    case kEqual:
-      *predicate = true;
-      return EQ;
-    case kUnequal:
-      *predicate = false;
-      return EQ;
-    case kUnsignedLessThan:
-      *predicate = true;
-      return OLT;
-    case kUnsignedGreaterEqual:
-      *predicate = false;
-      return OLT;
-    case kUnsignedLessEqual:
-      *predicate = true;
-      return OLE;
-    case kUnsignedGreaterThan:
-      *predicate = false;
-      return OLE;
-    default:
-      *predicate = true;
-      break;
-  }
-  UNREACHABLE();
-}
