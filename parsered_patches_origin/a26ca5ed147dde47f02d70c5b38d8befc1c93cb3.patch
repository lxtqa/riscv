-    case MSA_HALF:
-      MSA_I5_DF(h, kMSALanesHalf);
-      break;
-    case MSA_WORD:
-      MSA_I5_DF(w, kMSALanesWord);
-      break;
-    case MSA_DWORD:
-      MSA_I5_DF(d, kMSALanesDword);
-      break;
-    default:
-      UNREACHABLE();
-  }
-#undef MSA_I5_DF
