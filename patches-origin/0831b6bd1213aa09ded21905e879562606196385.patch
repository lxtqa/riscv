+constexpr bool Builtins::IsJSEntryVariant(Builtin builtin) {
+  switch (builtin) {
+    case Builtin::kJSEntry:
+    case Builtin::kJSConstructEntry:
+    case Builtin::kJSRunMicrotasksEntry:
+      return true;
+    default:
+      return false;
+  }
+  UNREACHABLE();
+}
